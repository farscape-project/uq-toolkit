                    {
                        "type": "csv",
                        "distribution": {"name":"uniform", "loc":0.9, "scale":0.2, "fraction":true},
                        "scale_all":{},
                    }
                }
            },
            "uq_log_name": "uq_log_thermal",
        },
        "Meshing/mesh_config.jsonc":
        {
            "type": "json",
