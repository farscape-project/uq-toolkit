            {
                "Functions":
                {
                    "ss316l-rx" :
                    {
                        "type": "csv",
                        "distribution": {"name":"uniform", "loc":0.9, "scale":0.2, "fraction":true},
                        "scale_all":{},
                    },
                },
                "Coefficients":
                {
                    "CurrentMagnitude":
                    {
                        "type": "value", // will parse "x" and "y" parameter from moose input
